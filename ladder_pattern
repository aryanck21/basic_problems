#include <iostream>

using namespace std;

int main()
{
    int n,i,j,k,l;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=2;j++)
        {
            cout<<"*   *"<<endl;
        }
        for(k=1;k<=5;k++)
        {
            cout<<"*";
        }
        cout<<endl;
    }
    for(l=1;l<=2;l++)                   //loop for last two rows
    {
        cout<<"*   *"<<endl;
    }

    return 0;
}
