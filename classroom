#include <iostream>

using namespace std;

struct clss
{
    int roll;
    char name[20];
    int marks;
};
int main()
{
    int i,count0=0,count1=0,count2=0,count3=0,count4=0,count5=0,count6=0,count7=0,count8=0,count9=0;
    clss student[20];
    cout<<"ENTER ROLL NO. ,NAME AND MARKS OF EACH STUDENT RESPECTIVELY\n";
    for (i=0;i<20;i++)
    {
        cin>>student[i].roll;
        cin>>student[i].name;
        cin>>student[i].marks;
        if(student[i].marks>=0&&student[i].marks<11)
        count0++;
        else if(student[i].marks>10&&student[i].marks<21)
        count1++;
        else if(student[i].marks>20&&student[i].marks<31)
        count2++;
        else if(student[i].marks>30&&student[i].marks<41)
        count3++;
        else if(student[i].marks>40&&student[i].marks<51)
        count4++;
        else if(student[i].marks>50&&student[i].marks<61)
        count5++;
        else if(student[i].marks>60&&student[i].marks<71)
        count6++;
        else if(student[i].marks>70&&student[i].marks<81)
        count7++;
        else if(student[i].marks>80&&student[i].marks<91)
        count8++;
        else if(student[i].marks>90&&student[i].marks<101)
        count9++;
        else NULL;
    }
    cout<<"ROLL NO.|NAME OF STUDENT   |MARKS\n";
    for(i=0;i<20;i++)
    {
        cout<<student[i].roll<<"\t|";
        cout<<student[i].name<<"\t\t|";
        cout<<student[i].marks<<"|\n";
    }
    cout<<"TOTAL CLASS FREQUENCY\n";
    cout<<"----------------------\n";
    cout<<"0  to 10 |"<<count0<<"\n";
    cout<<"11 to 20 |"<<count1<<"\n";
    cout<<"21 to 30 |"<<count2<<"\n";
    cout<<"31 to 40 |"<<count3<<"\n";
    cout<<"41 to 50 |"<<count4<<"\n";
    cout<<"51 to 60 |"<<count5<<"\n";
    cout<<"61 to 70 |"<<count6<<"\n";
    cout<<"71 to 80 |"<<count7<<"\n";
    cout<<"81 to 90 |"<<count8<<"\n";
    cout<<"91 to 100|"<<count9<<"\n";
    return 0;
    
}
