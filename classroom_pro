#include <iostream>

using namespace std;

struct clss
{
    char name[20];
    int math;
    int english;
    int hindi;
    int social;
    int science;
    int computer;
    int total;
};
int main()
{
    int i;
    int math_count0=0,math_count1=0,math_count2=0,math_count3=0,math_count4=0,math_count5=0,math_count6=0,math_count7=0,math_count8=0,math_count9=0;
    int eng_count0=0,eng_count1=0,eng_count2=0,eng_count3=0,eng_count4=0,eng_count5=0,eng_count6=0,eng_count7=0,eng_count8=0,eng_count9=0;
    int hindi_count0=0,hindi_count1=0,hindi_count2=0,hindi_count3=0,hindi_count4=0,hindi_count5=0,hindi_count6=0,hindi_count7=0,hindi_count8=0,hindi_count9=0;
    int sst_count0=0,sst_count1=0,sst_count2=0,sst_count3=0,sst_count4=0,sst_count5=0,sst_count6=0,sst_count7=0,sst_count8=0,sst_count9=0;
