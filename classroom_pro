                     /*FREQUENCY TABLE OF COMPUTER MARKS*/
    cout<<"_____________________________________________\n";
    cout<<"\tTOTAL CLASS FREQUENCY MARKS OF COMPUTER\n";
    cout<<"----------------------------------------------\n";
    cout<<"\n|  RANGE  |NUMBER OF STUDENTS\n";
    cout<<"|0  to 10 :"<<comp_count0<<"\n";
    cout<<"|11 to 20 :"<<comp_count1<<"\n";
    cout<<"|21 to 30 :"<<comp_count2<<"\n";
    cout<<"|31 to 40 :"<<comp_count3<<"\n";
    cout<<"|41 to 50 :"<<comp_count4<<"\n";
    cout<<"|51 to 60 :"<<comp_count5<<"\n";
    cout<<"|61 to 70 :"<<comp_count6<<"\n";
    cout<<"|71 to 80 :"<<comp_count7<<"\n";
    cout<<"|81 to 90 :"<<comp_count8<<"\n";
