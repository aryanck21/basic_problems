    int comp_count0=0,comp_count1=0,comp_count2=0,comp_count3=0,comp_count4=0,comp_count5=0,comp_count6=0,comp_count7=0,comp_count8=0,comp_count9=0;

    clss student[20];
    
                        /*LOOP TO ENTER DATA BEIGINS*/
                        
                        
    cout<<"ENTER ROLL NO.,NAME AND MARKS OF EACH STUDENT RESPECTIVELY\n";
    for (i=0;i<1;i++)
    {
        cout<<"\nENTER NAME OF ROLL NO."<<i+1;
        cin>>student[i].name;
                        /*TO ENETR MARKS IN MATHS AND TO COUNT ITS FREQUENCY*/
        cout<<"ENTER STUDENTS MARKS IN MATHS:";
        cin>>student[i].math;
        {
            if(student[i].math>=0&&student[i].math<11)
            math_count0++;
            else if(student[i].math>10&&student[i].math<21)
            math_count1++;
            else if(student[i].math>20&&student[i].math<31)
            math_count2++;
            else if(student[i].math>30&&student[i].math<41)
            math_count3++;
            else if(student[i].math>40&&student[i].math<51)
            math_count4++;
            else if(student[i].math>50&&student[i].math<61)
            math_count5++;
            else if(student[i].math>60&&student[i].math<71)
            math_count6++;
            else if(student[i].math>70&&student[i].math<81)
            math_count7++;
            else if(student[i].math>80&&student[i].math<91)
            math_count8++;
            else if(student[i].math>90&&student[i].math<101)
            math_count9++;
            else NULL;
        }
        
                        /*TO ENTER MARKS IN ENGLISH AND TO COUNT ITS FREQUENCY*/
        cout<<"ENTER STUDENT's MARKS IN ENGLISH:";
