                     /*FREQUENCY TABLE OF SCIENCE MARKS*/
    cout<<"___________________________________________\n";
    cout<<"\tTOTAL CLASS FREQUENCY MARKS OF SCIENCE\n";
    cout<<"----------------------------------------------\n";
    cout<<"\n|  RANGE  |NUMBER OF STUDENTS\n";
    cout<<"|0  to 10 :"<<sci_count0<<"\n";
    cout<<"|11 to 20 :"<<sci_count1<<"\n";
    cout<<"|21 to 30 :"<<sci_count2<<"\n";
    cout<<"|31 to 40 :"<<sci_count3<<"\n";
    cout<<"|41 to 50 :"<<sci_count4<<"\n";
