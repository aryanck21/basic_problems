            comp_count3++;
            else if(student[i].computer>40&&student[i].computer<51)
            comp_count4++;
            else if(student[i].computer>50&&student[i].computer<61)
            comp_count5++;
            else if(student[i].computer>60&&student[i].computer<71)
            comp_count6++;
            else if(student[i].computer>70&&student[i].computer<81)
            comp_count7++;
            else if(student[i].computer>80&&student[i].computer<91)
            comp_count8++;
            else if(student[i].computer>90&&student[i].computer<101)
            comp_count9++;
            else NULL;
        }
        
                        /*TO FIND THE TOTAL MARKS OF STUDENT*/ 
        student[i].total=student[i].math+student[i].english+student[i].hindi+student[i].social+student[i].science+student[i].computer;
    }
                        
                        /*LOOP TO PRINT SDUDENTS MARKS BEIGINS*/
                        
    cout<<"___________________________________________________________________________________\n";                  
