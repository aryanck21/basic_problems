    cout<<"|41 to 50 :"<<math_count4<<"\n";
    cout<<"|51 to 60 :"<<math_count5<<"\n";
    cout<<"|61 to 70 :"<<math_count6<<"\n";
    cout<<"|71 to 80 :"<<math_count7<<"\n";
    cout<<"|81 to 90 :"<<math_count8<<"\n";
    cout<<"|91 to 100:"<<math_count9<<"\n\n";
                     
                     /*FREQUENCY TABLE OF ENGLISH MARKS*/
    cout<<"___________________________________________\n";
    cout<<"\tTOTAL CLASS FREQUENCY MARKS OF ENGLISH\n";
    cout<<"--------------------------------------------\n";
    cout<<"\n|  RANGE  |NUMBER OF STUDENTS\n";
    cout<<"|0  to 10 :"<<eng_count0<<"\n";
