        cout<<student[i].hindi<<"\t|";
        cout<<student[i].social<<"\t|";
        cout<<student[i].science<<"\t|";
        cout<<student[i].computer<<"\t|";
        cout<<student[i].total<<"\t|\n\n";
    }
                        
                        /*FREQUENCY TABLE OF MATHS MARKS*/
    cout<<"_________________________________________\n";
    cout<<"\tTOTAL CLASS FREQUENCY MARKS OF MATHS\n";
    cout<<"------------------------------------------\n";
    cout<<"\n|  RANGE  |NUMBER OF STUDENTS \n";
