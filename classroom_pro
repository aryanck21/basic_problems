            sst_count5++;
            else if(student[i].social>60&&student[i].social<71)
            sst_count6++;
            else if(student[i].social>70&&student[i].social<81)
            sst_count7++;
            else if(student[i].social>80&&student[i].social<91)
            sst_count8++;
            else if(student[i].social>90&&student[i].social<101)
            sst_count9++;
